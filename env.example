DEEPSEEK_API_KEY=
BINANCE_API_KEY=
BINANCE_SECRET=

# --- 新增配置 ---
# 格式: SYMBOL1:LEVERAGE1,SYMBOL2:LEVERAGE2
# 例如: BTC/USDT:10,ETH/USDT:10,SOL/USDT:20
# 请注意，币种之间用逗号分隔，币种与杠杆之间用冒号分隔
SYMBOLS_AND_LEVERAGE=BTC/USDT:5,ETH/USDT:5,SOL/USDT:3

# 为所有币种统一设置K线周期和交易数量
DEFAULT_TIMEFRAME=15m
DEFAULT_AMOUNT=0.001 # 这是BTC的数量，对于ETH和SOL，数量会根据其价格调整，以保持相似的头寸价值
# 如果希望每个币种有独立的数量，请在TRADE_CONFIG中为每个symbol单独定义amount

# 测试模式: True 或 False (字符串)
# 如果设置为 True，则不会发送真实的交易订单
DEFAULT_TEST_MODE=True
